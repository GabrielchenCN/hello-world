<script>
    function numstr(str){

var obj={};
for(var i=0,l=str.length,k;i<l;i++){
	k=str.charAt(i);
	if(obj[k]){
		obj[k]++;
	}else{
		obj[k]=1;
	}
}
var m=0;
var i=null;
for(var k in obj){
	if(obj[k]>m){
		m=obj[k];
		i=k;
	}
}
alert(i+':'+m);

    }

    var a =numstr("sasaqwqwqewqqqq");

</script>
